{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@/*": [
        "src/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@compo/*": [
        "src/compo/*"
      ],
      "@images/*": [
        "src/assets/images/*"
      ],
      "@svgs/*": [
        "src/assets/svgs/*"
      ],
      "~assets/*": [
        "src/assets/*"
      ],
      "~components/*": [
        "src/components/*"
      ],
      "~pages/*": [
        "src/pages/*"
      ],
      "~routes/*": [
        "src/routes/*"
      ],
      "~/*": [
        "src/*"
      ]
    }
  }
}